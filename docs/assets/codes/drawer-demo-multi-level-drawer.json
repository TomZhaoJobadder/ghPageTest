mplate>\r\n        </nz-list>\r\n      </nz-drawer>\r\n    </nz-drawer>\r\n  `\r\n})\r\nexport class NzDemoDrawerMultiLevelDrawerComponent {\r\n  visible = false;\r\n  childrenVisible = false;\r\n\r\n  vegetables = ['asparagus', 'bamboo', 'potato', 'carrot', 'cilantro', 'potato', 'eggplant'];\r\n\r\n  open(): void {\r\n    this.visible = true;\r\n  }\r\n\r\n  close(): void {\r\n    this.visible = false;\r\n  }\r\n\r\n  openChildren(): void {\r\n    this.childrenVisible = true;\r\n  }\r\n\r\n  closeChildren(): void {\r\n    this.childrenVisible = false;\r\n  }\r\n}\r\n"}

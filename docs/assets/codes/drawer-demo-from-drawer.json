elector: 'nz-demo-drawer-from-drawer',\r\n  template: `\r\n    <button nz-button nzType=\"primary\" (click)=\"open()\">Create</button>\r\n    <nz-drawer\r\n      [nzBodyStyle]=\"{ overflow: 'auto' }\"\r\n      [nzMaskClosable]=\"false\"\r\n      [nzWidth]=\"720\"\r\n      [nzVisible]=\"visible\"\r\n      nzTitle=\"Create\"\r\n      [nzFooter]=\"footerTpl\"\r\n      (nzOnClose)=\"close()\"\r\n    >\r\n      <form nz-form *nzDrawerContent>\r\n        <div nz-row [nzGutter]=\"8\">\r\n          <div nz-col nzSpan=\"12\">\r\n            <nz-form-item>\r\n              <nz-form-label>Name</nz-form-label>\r\n              <nz-form-control>\r\n                <input nz-input placeholder=\"please enter user name\" />\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n          <div nz-col nzSpan=\"12\">\r\n            <nz-form-item>\r\n              <nz-form-label>Url</nz-form-label>\r\n              <nz-form-control>\r\n                <nz-input-group nzAddOnBefore=\"http://\" nzAddOnAfter=\".com\">\r\n                  <input type=\"text\" nz-input placeholder=\"please enter url\" />\r\n                </nz-input-group>\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n        </div>\r\n        <div nz-row [nzGutter]=\"8\">\r\n          <div nz-col nzSpan=\"12\">\r\n            <nz-form-item>\r\n              <nz-form-label>Owner</nz-form-label>\r\n              <nz-form-control>\r\n                <nz-select nzPlaceHolder=\"Please select an owner\"></nz-select>\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n          <div nz-col nzSpan=\"12\">\r\n            <nz-form-item>\r\n              <nz-form-label>Type</nz-form-label>\r\n              <nz-form-control>\r\n                <nz-select nzPlaceHolder=\"Please choose the type\"></nz-select>\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n        </div>\r\n        <div nz-row [nzGutter]=\"8\">\r\n          <div nz-col nzSpan=\"12\">\r\n            <nz-form-item>\r\n              <nz-form-label>Approver</nz-form-label>\r\n              <nz-form-control>\r\n                <nz-select nzPlaceHolder=\"Please choose the approver\"></nz-select>\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n          <div nz-col nzSpan=\"12\">\r\n            <nz-form-item>\r\n              <nz-form-label>DateTime</nz-form-label>\r\n              <nz-form-control>\r\n                <nz-range-picker></nz-range-picker>\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n        </div>\r\n        <div nz-row [nzGutter]=\"8\">\r\n          <div nz-col nzSpan=\"24\">\r\n            <nz-form-item>\r\n              <nz-form-label>Description</nz-form-label>\r\n              <nz-form-control>\r\n                <textarea\r\n                  nz-input\r\n                  placeholder=\"please enter url description\"\r\n                  [nzAutosize]=\"{ minRows: 4, maxRows: 4 }\"\r\n                ></textarea>\r\n              </nz-form-control>\r\n            </nz-form-item>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <ng-template #footerTpl>\r\n        <div style=\"float: right\">\r\n          <button nz-button style=\"margin-right: 8px;\" (click)=\"close()\">Cancel</button>\r\n          <button nz-button nzType=\"primary\" (click)=\"close()\">Submit</button>\r\n        </div>\r\n      </ng-template>\r\n    </nz-drawer>\r\n  `\r\n})\r\nexport class NzDemoDrawerFromDrawerComponent {\r\n  visible = false;\r\n\r\n  open(): void {\r\n    this.visible = true;\r\n  }\r\n\r\n  close(): void {\r\n    this.visible = false;\r\n  }\r\n}\r\n"}

ab nzTitle=\"Range Week\">\r\n        <nz-range-picker\r\n          nzInline\r\n          nzMode=\"week\"\r\n          [(ngModel)]=\"rangeDate\"\r\n          (ngModelChange)=\"onChange($event)\"\r\n        ></nz-range-picker>\r\n      </nz-tab>\r\n      <nz-tab nzTitle=\"Range Month\">\r\n        <nz-range-picker\r\n          nzInline\r\n          nzMode=\"month\"\r\n          [(ngModel)]=\"rangeDate\"\r\n          (ngModelChange)=\"onChange($event)\"\r\n        ></nz-range-picker>\r\n      </nz-tab>\r\n      <nz-tab nzTitle=\"Range Year\">\r\n        <nz-range-picker\r\n          nzInline\r\n          nzMode=\"year\"\r\n          [(ngModel)]=\"rangeDate\"\r\n          (ngModelChange)=\"onChange($event)\"\r\n        ></nz-range-picker>\r\n      </nz-tab>\r\n    </nz-tabset>\r\n  `,\r\n  styles: [\r\n    `\r\n      :host ::ng-deep .ant-tabs-tabpane {\r\n        padding: 24px;\r\n        overflow: auto;\r\n      }\r\n    `\r\n  ]\r\n})\r\nexport class NzDemoDatePickerInlineComponent {\r\n  date = null;\r\n  rangeDate = null;\r\n\r\n  onChange(result: Date): void {\r\n    console.log('onChange: ', result);\r\n  }\r\n\r\n  getWeek(result: Date): void {\r\n    console.log('week: ', getISOWeek(result));\r\n  }\r\n}\r\n"}

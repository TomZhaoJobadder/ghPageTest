er>\r\n    </nz-modal>\r\n  `\r\n})\r\nexport class NzDemoModalBasicComponent {\r\n  isVisible = false;\r\n\r\n  constructor() {}\r\n\r\n  showModal(): void {\r\n    this.isVisible = true;\r\n  }\r\n\r\n  handleOk(): void {\r\n    console.log('Button ok clicked!');\r\n    this.isVisible = false;\r\n  }\r\n\r\n  handleCancel(): void {\r\n    console.log('Button cancel clicked!');\r\n    this.isVisible = false;\r\n  }\r\n}\r\n"}
